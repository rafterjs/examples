{
  "name": "@rafterjs/examples-api",
  "description": "A JSON API server example",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:config && npm run build:lib && npm run build:index && npm run build:types",
    "build:config": "babel config --out-dir dist/config --extensions .js,.jsx,.ts,.tsx --source-maps",
    "build:lib": "babel lib --out-dir dist/lib --extensions .js,.jsx,.ts,.tsx --source-maps",
    "build:index": "babel index.ts --out-dir dist --extensions .js,.jsx,.ts,.tsx --source-maps",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf ./dist",
    "lint": "eslint \"./lib/**/*\" \"config/**/*\" ./babel.config.js",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "@rafterjs/cors-plugin": "0.7.1",
    "@rafterjs/logger-plugin": "0.7.1",
    "@types/express": "4.17.11",
    "@types/express-serve-static-core": "4.17.19",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "express-http-context": "1.2.4",
    "helmet": "4.4.1",
    "log4js": "6.3.0",
    "rafter": "0.7.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@rafterjs/babel-preset-rafter": "0.7.1",
    "@rafterjs/eslint-config": "0.7.1",
    "@types/uuid": "8.3.0"
  }
}
